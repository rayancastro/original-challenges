{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "no-alert": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off",
    "class-methods-use-this": "off"
  },
  "env": {
    "node": true,
    "mocha": true,
    "browser": true
  }
}
